s=\"card-img-top\" :src=answer.pic.value>\n                <div class=\"card-body\">\n                    <h5 v-if=\"answer.item_label\" class=\"card-title\">{{ answer.item_label.value }}</h5>\n                    <h4 v-if=\"answer.item.type=='literal'\">{{answer.item.value}}</h4>\n                    <a :href=answer.item.value v-else >{{answer.item.value}}</a>\n                    <p class=\"card-text\" v-if=\"answer.item_description\">{{ answer.item_description.value }}</p>\n                </div>\n            </div>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport SearchService from \"../services/SearchService\";\n\nexport default {\n  name: \"main-search\",\n  data() {\n    return {\n      answers: [],\n      currentAnswer: null,\n      currentIndex: -1,\n      query:null,\n    };\n  },\n  methods: {\n    searchAnswer() {\n      this.clearSearchResults();\n      SearchService.searchAnswer(this.query)\n        .then(response => {\n          this.answers = response.data;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n    clearSearchResults(){\n      this.answers = null;\n    }\n  },\n  mounted() {\n    //this.retrieveTutorials();\n  }\n};\n</script>\n\n<style>\n.list {\n  text-align: left;\n  max-width: 750px;\n  margin: auto;\n}\n</style>\n"]}]}